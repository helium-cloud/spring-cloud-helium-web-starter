<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coral.mini.program.manager.business.dao.mapper.AppStatisMapper">

    <select id="findByLastCount" resultType="com.coral.mini.program.manager.business.entity.AppStatis">
        SELECT * FROM tap_statis ORDER BY create_time DESC limit 0, #{count}
    </select>
</mapper>